  },
  "homepage": "https://github.com/jonocairns/dankbot#readme",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.7",
    "discord.js": "^14.11.0",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^5.1.0",
    "libsodium-wrappers": "^0.7.10",
    "node-fetch": "^2.6.6",
