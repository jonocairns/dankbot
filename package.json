{
  "name": "dankbot",
  "version": "1.0.1",
  "description": "A dank discord bot",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint **/*.js",
    "test": "ava **/*-spec.js --verbose",
    "start": "node src/index.js",
    "prep": "node_modules/.bin/eslint **/*.js --fix && ava **/*-spec.js --verbose",
    "download": "node src/lib/download.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonocairns/dankbot.git"
  },
  "keywords": [
    "discordjs",
    "bot"
  ],
  "author": "Jono Cairns",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonocairns/dankbot/issues"
  },
  "homepage": "https://github.com/jonocairns/dankbot#readme",
  "dependencies": {
    "ava": "0.16.0",
    "aws-sdk": "2.6.12",
    "discord.js": "11.4.2",
    "dotenv": "6.1.0",
    "erlpack": "hammerandchisel/erlpack",
    "node-opus": "^0.2.7",
    "promise": "8.0.3",
    "request": "2.88.0",
    "sinon": "7.1.1",
    "uws": "9.14.0",
    "ytdl-core": "0.28.0"
  },
  "engines": {
    "node": "8.9.0",
    "npm": "5.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.16.3",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-2"
      ]
    }
  }
}
