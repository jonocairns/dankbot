{
	"name": "dankbot",
	"version": "1.0.1",
	"description": "A dank discord bot",
	"main": "dist/index.js",
	"scripts": {
		"test": "echo 'do this'",
		"start": "nodemon",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"build": "tsc",
		"docker:build": "docker build . -t jonocairns/dankbot",
		"docker:run": "docker run --env-file=.env -d jonocairns/dankbot:latest",
		"docker:push": "docker push jonocairns/dankbot:latest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonocairns/dankbot.git"
	},
	"keywords": [
		"discordjs",
		"bot"
	],
	"author": "Jono Cairns",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jonocairns/dankbot/issues"
	},
	"homepage": "https://github.com/jonocairns/dankbot#readme",
	"dependencies": {
		"@discordjs/opus": "^0.10.0",
		"@discordjs/voice": "^0.19.0",
		"@google/genai": "^1.27.0",
		"@snazzah/davey": "^0.1.7",
		"discord.js": "^14.11.0",
		"dotenv": "^8.2.0",
		"ffmpeg-static": "^5.1.0",
		"libsodium-wrappers": "^0.7.10",
		"node-fetch": "^2.6.6",
		"openai": "^6.6.0",
		"winston": "^3.8.2",
		"youtubei.js": "^16.0.1"
	},
	"engines": {
		"node": ">=22.0.0",
		"npm": ">=10.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.0",
		"@types/dotenv": "^8.2.0",
		"@types/node": "^15.12.4",
		"@types/node-fetch": "^2.6.4",
		"@types/winston": "^2.4.4",
		"@vercel/node": "^2.5.26",
		"nodemon": "^2.0.20",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.3.0"
	},
	"packageManager": "yarn@4.10.3"
}
